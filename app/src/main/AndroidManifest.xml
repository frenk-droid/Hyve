<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.login_example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Hyve"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Login_example">
        <activity android:name=".PostActivity"></activity>
        <activity android:name=".NewTopic" />
        <activity
            android:name=".HomepageActivity"
            android:theme="@style/Theme.Login_example.NoActionBar" />
        <activity
            android:name=".RegisterPage"
            android:label="@string/title_activity_register_page"
            android:theme="@style/Theme.Login_example.NoActionBar" />
        <activity
            android:name=".Login"
            android:theme="@style/Theme.Login_example.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>